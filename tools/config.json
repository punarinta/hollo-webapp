{
  "root": "..",
  "index": "index.html",
  "js":
  [
    "js-transpiled/vendy/md5",
    "js-transpiled/autosize",
    "js-transpiled/core",
    "js-transpiled/auth",
    "js-transpiled/contacts",
    "js-transpiled/chat-roster",
    "js-transpiled/subject-picker",
    "js-transpiled/messages",
    "js-transpiled/composer",
    "js-transpiled/settings",
    "js-transpiled/main"
  ],
  "mods":
  [
    "emojis",
    "blowfish"
  ],
  "css":
  {
    "files":
    [
      "css/main"
    ],
    "prefixes":
    [
      "-webkit-",
      "-moz-"
    ],
    "prefixed":
    [
      ["user-select", [0,1]],
      ["tap-highlight-color", [0,1]],
      ["animation", [0]],
      ["animation-fill-mode", [0]],
      ["animation-timing-function", [0]],
      ["animation-delay", [0]],
      ["animation-duration", [0]]
    ]
  },
  "dist": "../dist",
  "pre-shell":
  [
    "npm run build",
    "stylus css/stylus/main.styl -o css/main.css"
  ],
  "post-shell":
  [
    "cp favicon.ico $distDir/favicon.ico",
    "cp -R gfx $distDir/gfx",
    "cp -R favicon $distDir/favicon",
    "rm -rf js-transpiled"
  ]
}
