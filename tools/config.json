{
  "root": "..",
  "index": "index.html",
  "js":
  [
    "js/vendy/md5",
    "js/autosize",
    "js/core",
    "js/auth",
    "js/contacts",
    "js/messages",
    "js/composer",
    "js/settings",
    "js/main"
  ],
  "mods":
  [
    "emojis",
    "blowfish"
  ],
  "css":
  {
    "files":
    [
      "css/main"
    ],
    "prefixes":
    [
      "-webkit-",
      "-moz-"
    ],
    "prefixed":
    [
      ["user-select", [0,1]],
      ["tap-highlight-color", [0,1]],
      ["animation", [0]],
      ["animation-fill-mode", [0]],
      ["animation-timing-function", [0]],
      ["animation-delay", [0]]
    ]
  },
  "dist": "../dist",
  "pre-shell":
  [
    "stylus css/stylus/main.styl -o css/main.css"
  ],
  "post-shell":
  [
    "cp favicon.ico $distDir/favicon.ico",
    "cp -R gfx $distDir/gfx"
  ]
}
